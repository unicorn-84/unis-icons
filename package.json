{
  "name": "@web-uni/unis-icons",
  "version": "0.0.0-development",
  "description": "SVG-based ReactJS icon components",
  "type": "module",
  "exports": {
    ".": "./lib/index.js"
  },
  "main": "./lib/index.js",
  "files": [
    "lib",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "reactjs",
    "icons",
    "svg",
    "components"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": "git@github.com:unicorn-84/unis-icons.git",
  "author": "unicorn-84 <unicorn-84@ya.ru> (https://github.com/unicorn-84)",
  "bugs": {
    "url": "https://github.com/unicorn-84/unis-icons/issues"
  },
  "homepage": "https://unicorn-84.github.io/unis-icons",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "jest --passWithNoTests --silent",
    "test:ci": "jest --coverage --ci --passWithNoTests",
    "prebuild": "rimraf lib",
    "build": "tsc",
    "format": "prettier . -c",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "demo:dev": "cd demo && yarn dev",
    "demo:build": "cd demo && yarn build",
    "demo:preview": "cd demo && yarn preview",
    "semantic-release": "semantic-release",
    "cm": "cz",
    "prepare": "husky install",
    "svgr": "svgr --config-file ./src/svgr/svgr-config.json --index-template ./src/svgr/svgr-index-template.cjs --template ./src/svgr/svgr-template.cjs -- assets",
    "generate": "node ./scripts && yarn test"
  },
  "devDependencies": {
    "@svgr/cli": "6.3.1",
    "@testing-library/dom": "8.16.0",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
    "@types/jest": "28.1.6",
    "@types/jest-axe": "3.5.4",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.31.0",
    "@typescript-eslint/parser": "5.31.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-testing-library": "5.5.1",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "jest-axe": "6.0.0",
    "jest-environment-jsdom": "28.1.3",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "3.0.2",
    "semantic-release": "^19.0.3",
    "ts-jest": "28.0.7",
    "typescript": "4.7.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
