{
  "name": "@web-uni/unis-icons",
  "version": "0.0.0-development",
  "description": "SVG-based ReactJS icon components",
  "type": "module",
  "exports": {
    ".": "./lib/index.js"
  },
  "main": "./lib/index.js",
  "files": [
    "lib",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "reactjs",
    "icons",
    "svg",
    "components"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": "git@github.com:unicorn-84/unis-icons.git",
  "author": "unicorn-84 <unicorn-84@ya.ru> (https://github.com/unicorn-84)",
  "bugs": {
    "url": "https://github.com/unicorn-84/unis-icons/issues"
  },
  "homepage": "https://unicorn-84.github.io/unis-icons",
  "license": "MIT",
  "private": false,
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "tsc",
    "format": "prettier . -c",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "demo:dev": "cd demo && yarn dev",
    "demo:build": "cd demo && yarn && yarn build",
    "demo:preview": "cd demo && yarn preview",
    "semantic-release": "semantic-release",
    "cm": "cz",
    "prepare": "husky install",
    "svgr": "svgr --config-file ./src/svgr/.svgrrc.cjs --index-template ./src/svgr/svgr-index-template.cjs --template ./src/svgr/svgr-template.cjs -- $ICONS_DIR",
    "generate": "dotenv-run-script svgr && node ./utils/createDemo && npx prettier ./demo/src/App.tsx -w",
    "make": "node ./utils/editIcons && yarn generate"
  },
  "devDependencies": {
    "@svgr/cli": "6.3.1",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.31.0",
    "@typescript-eslint/parser": "5.31.0",
    "change-case": "4.1.2",
    "cz-conventional-changelog": "3.3.0",
    "dotenv": "16.0.1",
    "dotenv-run-script": "0.2.0",
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.30.1",
    "husky": "8.0.1",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "3.0.2",
    "semantic-release": "^19.0.3",
    "svgo": "2.8.0",
    "typescript": "4.7.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "camelcase": "7.0.0"
  }
}
